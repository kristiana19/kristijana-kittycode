	 "gitDecoration.conflictingResourceForeground": "#ff0033", // Konflikti

	 

	},


	"tokenColors": [
	  {
		"name": "Variables",
		"scope": [
		  "variable",
		  "string constant.other.placeholder",
		  "meta.definition.variable"
		],
		"settings": {
		  "foreground": "#f04ab8"
		}
	  },
	  {
		"name": "Comment",
		"scope": [
		  "comment",
		  "punctuation.definition.comment"
		],
		"settings": {
		  "foreground": "#888888",
		  "fontStyle": "italic"
		}
	  },
	  {
		"name": "Keywords",
		"scope": [
		  "keyword",
		  "storage.type",
		  "storage.modifier"
		],
		"settings": {
		  "foreground": "#f31fac",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "Strings",
		"scope": [
		  "string",
		  "constant.other.symbol",
		  "constant.other.key",
		  "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
		],
		"settings": {
		  "foreground": "#e6a2f7"
		}
	  },
	  {
		"name": "Terminal Errors",
		"scope": [
		  "markup.error",
		  "message.error",
		  "invalid"
		],
		"settings": {
		  "foreground": "#ff4d6d",
		  "fontStyle": "bold"
		}
	  }
	]
  }
  